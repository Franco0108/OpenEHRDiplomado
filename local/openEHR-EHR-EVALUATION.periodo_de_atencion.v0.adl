definition
	EVALUATION[at0000] matches {    -- Periodo de Atencion
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0003] matches {    -- Capacidad de Completar Pensamiento
						value matches {
							DV_BOOLEAN matches {*}
						}
					}
					ELEMENT[at0004] matches {    -- Capacidad de Resolver Problemas
						value matches {
							DV_BOOLEAN matches {*}
						}
					}
					ELEMENT[at0005] matches {    -- Capacidad de realizar operaciones matematicas secuenciales
						value matches {
							DV_BOOLEAN matches {*}
						}
					}
					ELEMENT[at0006] matches {    -- Deletro de palabras
						value matches {
							DV_BOOLEAN matches {*}
						}
					}
					ELEMENT[at0007] matches {    -- Repetir 7 numeros en orden ascendente y 5 en orden inverso
						value matches {
							DV_BOOLEAN matches {*}
						}
					}
				}
			}
		}
	}

