definition
	OBSERVATION[at0000] matches {    -- UsoMusculos
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					INTERVAL_EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0004] matches {    -- Presion Inspiratoria Maxima
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::125]>
												list = <
													["1"] = <
														units = <"cm[H2O]">
														precision = <|1|>
														units_system = <"http://unitsofmeasure.org">
														units_display_name = <"cmH₂O">
													>
												>
											>
										}
									}
									ELEMENT[at0005] matches {    -- Presion Expiratoria Maxima
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::125]>
												list = <
													["1"] = <
														units = <"cm[H2O]">
														precision = <|1|>
														units_system = <"http://unitsofmeasure.org">
														units_display_name = <"cmH₂O">
													>
												>
											>
										}
									}
									ELEMENT[at0006] matches {    -- Ventilacion Voluntaria Maxima
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0007] matches {    -- Prueba de Inhalacion Rapida
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0009] occurrences matches {0..1} matches {    -- Fecha de Registro
										value matches {
											DV_DATE_TIME matches {*}
										}
									}
								}
							}
						}
						width matches {
							DV_DURATION matches {
								value matches {PT6S}
							}
						}
					}
				}
			}
		}
	}

