definition
	EVALUATION[at0000] matches {    -- Actividad Psicomotora
		data matches {
			ITEM_TREE[at0001] matches {    -- Item tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002] matches {    -- Estado Emocional
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0003,    -- Tranquilo
									at0004,    -- Irritable
									at0005]    -- Ansioso
								}
							}
						}
					}
					ELEMENT[at0006] matches {    -- Expresion Corporal
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0007,    -- Tiene una expresión y movimiento corporal norma
									at0008]    -- Muestra un estado afectivo plano y deprimido
								}
							}
						}
					}
				}
			}
		}
	}

