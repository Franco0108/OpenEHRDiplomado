definition
	OBSERVATION[at0000] matches {    -- Signos Vitales
		protocol matches {
			ITEM_TREE[at0007] matches {*}    -- Item tree
		}
		data matches {
			HISTORY[at0001] matches {    -- History
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {    -- Any event
						data matches {
							ITEM_TREE[at0003] matches {    -- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0004] matches {    -- Temperatura
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::127]>
												list = <
													["1"] = <
														units = <"Cel">
														precision = <|1|>
														units_system = <"http://unitsofmeasure.org">
														units_display_name = <"Â°C">
													>
												>
											>
										}
									}
									ELEMENT[at0005] matches {    -- SpO2
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::507]>
												list = <
													["1"] = <
														units = <"%">
														precision = <|0|>
													>
												>
											>
										}
									}
									ELEMENT[at0006] matches {    -- Frecuencia Cardiaca
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::382]>
												list = <
													["1"] = <
														units = <"/min">
														precision = <|0|>
													>
												>
											>
										}
									}
									ELEMENT[at0008] matches {    -- Presion Sistolica
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::125]>
												list = <
													["1"] = <
														units = <"mm[Hg]">
														precision = <|0|>
														units_system = <"http://unitsofmeasure.org">
														units_display_name = <"mmHg">
													>
												>
											>
										}
									}
									ELEMENT[at0009] matches {    -- Presion Diastolica
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::125]>
												list = <
													["1"] = <
														units = <"mm[Hg]">
														precision = <|0|>
														units_system = <"http://unitsofmeasure.org">
														units_display_name = <"mmHg">
													>
												>
											>
										}
									}
									ELEMENT[at0010] matches {    -- Registro
										value matches {
											DV_DATE_TIME matches {*}
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}

